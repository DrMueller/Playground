<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MMU.TextFunctions.GUI.ViewModels.Shell">

    <DataTemplate x:Key="CommandViewModelButtonTemplate" DataType="{x:Type vm:ViewModelCommand}">
        <Button Margin="10,0,0,0" Command="{Binding Command}" Content="{Binding DisplayName}" ToolTip="{Binding ToolTipText}" />
    </DataTemplate>

</ResourceDictionary>