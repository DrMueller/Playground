<Window x:Class="MMU.WpfFunctionalities.TestWpfUI.MouseMoveCommandBindingTest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MMU.WpfFunctionalities.TestWpfUI"
        xmlns:xf="clr-namespace:MMU.BoerseDownloader.WpfUI.Infrastructure.XamlFunctionalities"
        mc:Ignorable="d"
        x:Name="win"
        Title="MouseMoveCommandBindingTest" Height="300" Width="300">
    <Grid Background="AliceBlue"
        xf:MouseMoveCommandBinding.MouseMoveCommand="{Binding ElementName=win, Path=MoveCommand}" 
        xf:MouseMoveCommandBinding.MouseMoveCommandParameter="{Binding ElementName=win, Path=CommandParameter}">
        
    </Grid>
</Window>
